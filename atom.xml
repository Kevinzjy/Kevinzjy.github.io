    <summary type="html">
    
      &lt;h2 id=&quot;ggsci颜色设置&quot;&gt;&lt;a href=&quot;#ggsci颜色设置&quot; class=&quot;headerlink&quot; title=&quot;ggsci颜色设置&quot;&gt;&lt;/a&gt;ggsci颜色设置&lt;/h2&gt;&lt;figure class=&quot;highlight r&quot;&gt;&lt;table&gt;&lt;tr&gt;&lt;td class=&quot;gutter&quot;&gt;&lt;pre&gt;&lt;div class=&quot;line&quot;&gt;1&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;2&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;3&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;4&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;5&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;6&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;7&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;8&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;9&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;10&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;11&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;12&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;13&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;14&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;15&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;16&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;17&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;18&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;19&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;20&lt;/div&gt;&lt;/pre&gt;&lt;/td&gt;&lt;td class=&quot;code&quot;&gt;&lt;pre&gt;&lt;div class=&quot;line&quot;&gt;&lt;span class=&quot;keyword&quot;&gt;library&lt;/span&gt;(&lt;span class=&quot;string&quot;&gt;&quot;ggsci&quot;&lt;/span&gt;)&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;&lt;span class=&quot;keyword&quot;&gt;library&lt;/span&gt;(&lt;span class=&quot;string&quot;&gt;&quot;ggplot2&quot;&lt;/span&gt;)&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;&lt;span class=&quot;keyword&quot;&gt;library&lt;/span&gt;(&lt;span class=&quot;string&quot;&gt;&quot;gridExtra&quot;&lt;/span&gt;)&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;data(&lt;span class=&quot;string&quot;&gt;&quot;diamonds&quot;&lt;/span&gt;)&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;p1 = ggplot(subset(diamonds, carat &amp;gt;= &lt;span class=&quot;number&quot;&gt;2.2&lt;/span&gt;),&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;       aes(x = table, y = price, colour = cut)) +&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;  geom_point(alpha = &lt;span class=&quot;number&quot;&gt;0.7&lt;/span&gt;) +&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;  geom_smooth(method = &lt;span class=&quot;string&quot;&gt;&quot;loess&quot;&lt;/span&gt;, alpha = &lt;span class=&quot;number&quot;&gt;0.05&lt;/span&gt;, size = &lt;span class=&quot;number&quot;&gt;1&lt;/span&gt;, span = &lt;span class=&quot;number&quot;&gt;1&lt;/span&gt;) +&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;  theme_bw()&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;p2 = ggplot(subset(diamonds, carat &amp;gt; &lt;span class=&quot;number&quot;&gt;2.2&lt;/span&gt; &amp;amp; depth &amp;gt; &lt;span class=&quot;number&quot;&gt;55&lt;/span&gt; &amp;amp; depth &amp;lt; &lt;span class=&quot;number&quot;&gt;70&lt;/span&gt;),&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;       aes(x = depth, fill = cut)) +&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;  geom_histogram(colour = &lt;span class=&quot;string&quot;&gt;&quot;black&quot;&lt;/span&gt;, binwidth = &lt;span class=&quot;number&quot;&gt;1&lt;/span&gt;, position = &lt;span class=&quot;string&quot;&gt;&quot;dodge&quot;&lt;/span&gt;) +&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;  theme_bw()&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;p1_npg = p1 + scale_color_npg()&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;p2_npg = p2 + scale_fill_npg()&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;grid.arrange(p1_npg, p2_npg, ncol = &lt;span class=&quot;number&quot;&gt;2&lt;/span&gt;)&lt;/div&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
    
    </summary>
    
