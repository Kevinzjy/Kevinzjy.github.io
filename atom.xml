ss=&quot;line&quot;&gt;&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;p1 = ggplot(subset(diamonds, carat &amp;gt;= &lt;span class=&quot;number&quot;&gt;2.2&lt;/span&gt;),&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;       aes(x = table, y = price, colour = cut)) +&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;  geom_point(alpha = &lt;span class=&quot;number&quot;&gt;0.7&lt;/span&gt;) +&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;  geom_smooth(method = &lt;span class=&quot;string&quot;&gt;&quot;loess&quot;&lt;/span&gt;, alpha = &lt;span class=&quot;number&quot;&gt;0.05&lt;/span&gt;, size = &lt;span class=&quot;number&quot;&gt;1&lt;/span&gt;, span = &lt;span class=&quot;number&quot;&gt;1&lt;/span&gt;) +&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;  theme_bw()&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;p2 = ggplot(subset(diamonds, carat &amp;gt; &lt;span class=&quot;number&quot;&gt;2.2&lt;/span&gt; &amp;amp; depth &amp;gt; &lt;span class=&quot;number&quot;&gt;55&lt;/span&gt; &amp;amp; depth &amp;lt; &lt;span class=&quot;number&quot;&gt;70&lt;/span&gt;),&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;       aes(x = depth, fill = cut)) +&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;  geom_histogram(colour = &lt;span class=&quot;string&quot;&gt;&quot;black&quot;&lt;/span&gt;, binwidth = &lt;span class=&quot;number&quot;&gt;1&lt;/span&gt;, position = &lt;span class=&quot;string&quot;&gt;&quot;dodge&quot;&lt;/span&gt;) +&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;  theme_bw()&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;p1_npg = p1 + scale_color_npg()&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;p2_npg = p2 + scale_fill_npg()&lt;/div&gt;&lt;div class=&quot;line&quot;&gt;grid.arrange(p1_npg, p2_npg, ncol = &lt;span class=&quot;number&quot;&gt;2&lt;/span&gt;)&lt;/div&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
    
    </summary>
    
